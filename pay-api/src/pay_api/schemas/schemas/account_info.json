{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://bcrs.gov.bc.ca/.well_known/schemas/account_info",
  "type": "object",
  "required": [
  ],
  "properties": {
    "routingSlip": {
      "$id": "#/properties/routingSlip",
      "type": "string",
      "title": "Routing Slip Number",
      "examples": [
        "ABCD123456789"
      ],
      "pattern": "^(.*)$"
    },
    "datNumber": {
      "$id": "#/properties/datNumber",
      "type": "string",
      "title": "DAT Number for BCOL",
      "examples": [
        "C1234567890"
      ],
      "pattern": "^(.*)$"
    },
    "bcolAccountNumber": {
      "$id": "#/properties/bcolAccountNumber",
      "type": "string",
      "title": "BC Online Account Number",
      "examples": [
        "180670"
      ],
      "pattern": "^(.*)$"
    }
  }
}